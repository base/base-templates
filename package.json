{
  "name": "base-templates",
  "description": "Plugin for adding Templates support to base applications.",
  "version": "0.2.0",
  "homepage": "https://github.com/node-base/base-templates",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "node-base/base-templates",
  "bugs": {
    "url": "https://github.com/node-base/base-templates/issues"
  },
  "license": "MIT",
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "base-option": "^0.8.4",
    "base-plugins": "^0.4.13",
    "is-plain-object": "^2.0.1",
    "is-valid-app": "^0.2.0",
    "lazy-cache": "^2.0.1",
    "templates": "^0.25.1"
  },
  "devDependencies": {
    "base": "^0.11.1",
    "base-generators": "^0.4.5",
    "base-test-runner": "^0.2.0",
    "base-test-suite": "^0.2.4",
    "engine-handlebars": "^0.8.0",
    "gulp-format-md": "^0.1.10",
    "yargs-parser": "^3.2.0"
  },
  "keywords": [
    "api",
    "app",
    "application",
    "base",
    "baseplugin",
    "building-blocks",
    "create",
    "framework",
    "plugin",
    "plugins",
    "templates",
    "tool",
    "toolkit",
    "tools"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "templates",
        "assemble",
        "generate",
        "update"
      ]
    },
    "reflinks": [
      "verb",
      "templates",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}

{
  "name": "base-templates",
  "description": "Plugin for adding Templates support to base applications.",
  "version": "0.1.0",
  "homepage": "https://github.com/node-base/base-templates",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "node-base/base-templates",
  "bugs": {
    "url": "https://github.com/node-base/base-templates/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "LICENSE",
    "README.md",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "define-property": "^0.2.5",
    "is-valid-app": "^0.2.0",
    "lazy-cache": "^2.0.1",
    "mixin-deep": "^1.1.3",
    "templates": "^0.24.0"
  },
  "devDependencies": {
    "base": "^0.11.1",
    "base-test-runner": "^0.2.0",
    "base-test-suite": "^0.1.12",
    "engine-handlebars": "^0.8.0",
    "gulp-format-md": "^0.1.9",
    "yargs-parser": "^2.4.0"
  },
  "keywords": [
    "api",
    "app",
    "application",
    "base",
    "baseplugin",
    "building-blocks",
    "create",
    "framework",
    "plugin",
    "plugins",
    "templates",
    "tool",
    "toolkit",
    "tools"
  ],
  "lintDeps": {
    "ignore": []
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": false,
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}